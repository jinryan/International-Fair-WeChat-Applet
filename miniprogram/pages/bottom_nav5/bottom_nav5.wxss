.header_alt{
  display: block;
  margin-left: auto;
  margin-right: auto;
  width: 100%;
  height: 80px;
  position: fixed;
  z-index: 5;
  box-shadow: 0px 0px 5px #808080;
}

.filler{
  display: block;
  background-color: #f1f1f1;
  width: 100%;
  height: 80px; 
}

.whiteContainer {
  position: relative; display:inline-block; z-index:3; background: white;   width: 100%; color: #392323; box-shadow: 0px 0px 5px #d3d3d3;
  margin-left: auto;
  margin-right: auto;
  margin-top: 10px;
}

.containerImage{
  position: absolute;
  top: 20rpx;
  left: 20rpx;
  height: 510rpx;
  width: 50%;
  border-radius: 10px;
  overflow: hidden;
}

.containerText{
  position: absolute;
  top: 20rpx;
  right: 20rpx;
  height: 510rpx;
  width: 40%;
  text-align: left;
  font-family: CO;
  font-size: 14px;
}

.headerText{
  font-family: CBO; font-size: 20px;
  /* margin-top: 10rpx; */
}

.redButton{
position: absolute; 
right: 30rpx;
bottom: 30rpx; 
width: 120px; 
height: 30px; 
border-radius: 15px; 
text-align: center; 
color: #c4000a; 
font-family: COI; 
border: 1px solid rgba(220, 0, 0, 1)
}