Page {
  --bg-color: white; 
}
.table {
  border: 0px solid darkgray;
}
.header_alt{
  display: inline-block;
  margin-left: auto;
  margin-right: auto;
  width: 100%;
  height: 80px;
  position: relative;
  z-index: 10;
  box-shadow: 0px 0px 5px #808080;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
  font-size: 15px;
}
.td {
    width:40%;
    justify-content: center;
    text-align: center;
}
.th {
  width: 40%;
  justify-content: center;
  display: flex;
  height: 3rem;
  align-items: center;
}
.center {
  display: block;
  margin-left: auto;
  margin-right: auto;
  width: 50%;
}

.whiteButton {
  position: relative; display:inline-block; z-index:3; background: white;   width: 50%; color: #392323; box-shadow: 0px 0px 5px #d3d3d3;
  margin-left: auto;
  margin-right: auto;
  margin-top: 12px;
}

