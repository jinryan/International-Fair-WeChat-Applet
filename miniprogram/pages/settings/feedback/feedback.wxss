Page {
  --bg-color: white; 
}

.container {
  position: relative;
  display:inline-block;
  z-index:3;
  background: white;
  width: 100%;
  color: #392323;
  /* box-shadow: 0px 0px 5px #d3d3d3; */
  margin-left: auto;
  margin-right: auto;
  margin-top: 12px;
  bottom: 0px;
}
.center {
  display: block;
  margin-left: auto;
  margin-right: auto;
  position: relative;
  width: 92%;
}

.header_alt{
  display: block;
  margin-left: auto;
  margin-right: auto;
  width: 100%;
  height: 80px;
  position: fixed;
  z-index: 5;
  box-shadow: 0px 0px 5px #808080;
}

.filler{
  display: block;
  background-color: #f2f2f2;
  width: 100%;
  height: 80px;
}

.submitButton{
  font-family: COI;
  width: 40%; 
  background: #FFFFFF; 
  border-radius: 5px;
  border: 1px solid rgba(220, 0, 0, 1);
  text-align: center; 
  color: #c4000a;
  text-size: 15px;
}