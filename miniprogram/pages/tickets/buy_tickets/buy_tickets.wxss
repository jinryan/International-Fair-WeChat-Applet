.navbar {
  overflow: hidden;
  background-color: #f5f5f5;
  position: fixed;
  bottom: 0;
  width: 100%;
}

.navbar button {
  float: left;
  display: block;
  color: #f2f2f2;
  text-align: center;
  padding: 14px 16px;
  text-decoration: none;
  font-size: 17px;
  width: 20%;
}

.navbar button:hover {
  background: #f1f1f1;
  color: black;
}

.navbar button.active {
  background-color: #4CAF50;
  color: white;
}

.nav_icon{
  width: 40px;
  height: 40px;
}

.header_alt{
  display: block;
  margin-left: auto;
  margin-right: auto;
  width: 100%;
  height: 80px;
  position: fixed;
  z-index: 5;
  box-shadow: 0px 0px 5px #808080;
}

.filler{
  display: block;
  background-color: #f1f1f1;
  width: 100%;
  height: 85px;
}

.buyTicket{
  position: relative;
  width:250rpx;
  height:330rpx;
}

.column {
  float: left;
  width: 50%;
}

/* Clear floats after the columns */
.row:after {
  content: "";
  display: table;
  clear: both;
}

.ticketEagle{
  display: block;
  margin-left: auto;
  margin-right: auto;
  width:400rpx;
  height:400rpx;
}

.centerHeader{
  display: block;
  margin-left: auto;
  margin-right: auto;
  text-align: center;
  font-size: 20px;

}

.center{
  display: block;
  margin-left: auto;
  margin-right: auto;
  width: 80%;
}