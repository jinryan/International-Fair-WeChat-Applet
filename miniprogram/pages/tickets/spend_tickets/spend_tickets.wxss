Page {
  --bg-color: #ededed; 
}

.navbar {
  overflow: hidden;
  background-color: #f5f5f5;
  position: fixed;
  bottom: 0;
  width: 100%;
}

.navbar button {
  float: left;
  display: block;
  color: #f2f2f2;
  text-align: center;
  padding: 14px 16px;
  text-decoration: none;
  font-size: 17px;
  width: 20%;
}

.navbar button:hover {
  background: #f1f1f1;
  color: black;
}

.navbar button.active {
  background-color: #4CAF50;
  color: white;
}

.nav_icon{
  width: 40px;
  height: 40px;
}

.header_alt{
  display: block;
  margin-left: auto;
  margin-right: auto;
  width: 100%;
  height: 80px;
  position: fixed;
  z-index: 5;
}

.filler{
  display: block;
  background-color: #f2f2f2;
  width: 100%;
  height: 80px;
}

.center {
  display: block;
  margin-left: auto;
  margin-right: auto;
  width: 100%;
  position: relative;
}

.profile_pic{
  position:relative;
  height: 150rpx;
  width: 150rpx;
  left: 300rpx;
  top: 55rpx;
  border-radius: 10%;
}
.profile_view{
  height:250rpx;
}